  </requires>
  <extension point="xbmc.python.script" library="devicesettings.py">
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <minversion>1</minversion>
    <summary lang="en">AlienUI Device Settings</summary>
    <description lang="en">AlienUI Device Settings</description>
  </extension>
</addon>

