<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.helix.alienware" name="Alienware Helix Repo" version="0.0.16" provider-name="Alienware">
        <extension point="xbmc.addon.repository" name="Alienware's Helix addons">
                <info compressed="false">http://raw.github.com/alienwaresoftware/Kodi/master/Helix/addons.xml</info>
                <checksum>http://raw.github.com/alienwaresoftware/Kodi/master/Helix/addons.xml.md5</checksum>
                <datadir zip="true">http://raw.github.com/alienwaresoftware/Kodi/master/Helix</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
                <summary>Alienware's Helix Repo</summary>
                <description>Alienware's Helix Repo</description>
                <disclaimer></disclaimer>
                <platform>all</platform>
        </extension>
</addon>

