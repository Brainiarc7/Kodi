#Customizations

msgctxt "#33000"
msgid "Would you like to install The HiveMind Core Components?"
msgstr "Wilt u de kernonderdelen van The HiveMind installeren?"

msgctxt "#33001"
msgid "These components are required to receive all the benefits of The HiveMind."
msgstr "Deze onderdelen zijn nodig om te kunnen genieten van alle voordelen van The HiveMind."

msgctxt "#33002"
msgid "Please wait while installing HiveMind core components"
msgstr "Wacht terwijl HiveMind-kernonderdelen worden geïnstalleerd"

msgctxt "#33003"
msgid "Unable to install HiveMind core components"
msgstr "Kan HiveMind-kernonderdelen niet installeren"

msgctxt "#33004"
msgid "Exit Code = "
msgstr "Afsluitcode = "

msgctxt "#33005"
msgid "You will need to restart your computer to complete this installation."
msgstr "U moet uw computer opnieuw opstarten om deze installatie te voltooien."

msgctxt "#33006"
msgid "Would you like to restart the system now?"
msgstr "Wilt u het systeem nu opnieuw opstarten?"

msgctxt "#33007"
msgid "A new version of The HiveMind Core Components is now available!"
msgstr "Er is nu een nieuwe versie van de kernonderdelen van The HiveMind beschikbaar!"

msgctxt "#33008"
msgid "Would you like to upgrade now?"
msgstr "Wilt u nu upgraden?"

msgctxt "#33009"
msgid "Current Version"
msgstr "Huidige versie"

msgctxt "#33010"
msgid "New Version"
msgstr "Nieuwe versie"

msgctxt "#33011"
msgid "The HiveMind Core Components must be installed to run this addon."
msgstr "De hivemind Core Components moeten worden geïnstalleerd om deze addon lopen."