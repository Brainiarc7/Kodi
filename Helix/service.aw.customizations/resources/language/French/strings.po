#Customizations

msgctxt "#33000"
msgid "Would you like to install The HiveMind Core Components?"
msgstr "Voulez-vous installer les composants principaux de HiveMind ?"

msgctxt "#33001"
msgid "These components are required to receive all the benefits of The HiveMind."
msgstr "Ces composants sont nécessaires pour bénéficier de tous les avantages de HiveMind."

msgctxt "#33002"
msgid "Please wait while installing HiveMind core components"
msgstr "Veuillez patientez pendant l’installation des composants principaux d’HiveMind"

msgctxt "#33003"
msgid "Unable to install HiveMind core components"
msgstr "Échec de l’installation des composants principaux d’HiveMind"

msgctxt "#33004"
msgid "Exit Code = "
msgstr "Code de sortie = "

msgctxt "#33005"
msgid "You will need to restart your computer to complete this installation."
msgstr "Vous devez redémarrer votre ordinateur afin de compléter l’installation."

msgctxt "#33006"
msgid "Would you like to restart the system now?"
msgstr "Voulez-vous redémarrer maintenant ?"

msgctxt "#33007"
msgid "A new version of The HiveMind Core Components is now available!"
msgstr "Une nouvelle version des composants principaux de HiveMind est disponible!"

msgctxt "#33008"
msgid "Would you like to upgrade now?"
msgstr "Voulez-vous effectuer la mise à niveau maintenant ?"

msgctxt "#33009"
msgid "Current Version"
msgstr "Version actuelle"

msgctxt "#33010"
msgid "New Version"
msgstr "Nouvelle Version"

msgctxt "#33011"
msgid "The HiveMind Core Components must be installed to run this addon."
msgstr "Les composants HiveMind base doit être installé pour fonctionner cet addon."
