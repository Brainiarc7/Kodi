#Customizations

msgctxt "#33000"
msgid "Would you like to install The HiveMind Core Components?"
msgstr "¿Desea instalar los componentes principales de HiveMind?"

msgctxt "#33001"
msgid "These components are required to receive all the benefits of The HiveMind."
msgstr "Estos componentes son necesarios para obtener todas las ventajas de HiveMind."

msgctxt "#33002"
msgid "Please wait while installing HiveMind core components"
msgstr "Por favor, espere mientras se instalan los componentes principales de HiveMind"

msgctxt "#33003"
msgid "Unable to install HiveMind core components"
msgstr "No se han podido instalar los componentes principales de HiveMind"

msgctxt "#33004"
msgid "Exit Code = "
msgstr "Código de salida = "

msgctxt "#33005"
msgid "You will need to restart your computer to complete this installation."
msgstr "Debe reiniciar el equipo para finalizar la instalación."

msgctxt "#33006"
msgid "Would you like to restart the system now?"
msgstr "¿Desea reiniciar el sistema ahora?"

msgctxt "#33007"
msgid "A new version of The HiveMind Core Components is now available!"
msgstr "Hay una nueva versión disponible de los componentes principales de HiveMind"

msgctxt "#33008"
msgid "Would you like to upgrade now?"
msgstr "¿Desea actualizar ahora?"

msgctxt "#33009"
msgid "Current Version"
msgstr "Versión actual"

msgctxt "#33010"
msgid "New Version"
msgstr "Nueva versión"

msgctxt "#33011"
msgid "The HiveMind Core Components must be installed to run this addon."
msgstr "Los Componentes HiveMind Core debe estar instalado para ejecutar este complemento."
